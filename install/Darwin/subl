if [ -x "/Applications/Sublime Text.app/Contents/SharedSupport/bin/subl" ]; then
  if [ $debug = false ];
    then ln -s "/Applications/Sublime Text.app/Contents/SharedSupport/bin/subl" /usr/local/bin/subl
    else ln -s "/Applications/Sublime Text.app/Contents/SharedSupport/bin/subl" /usr/local/bin/subl
  fi
fi
