[alias]
  a = add
  ae = add --edit
  ap = add --patch
  b = branch
  br = branch
  c = commit
  ci = commit
  co = checkout
  cp = cherry-pick
  d = diff
  di = diff --cached HEAD
  df = diff
  f = fetch
  fl = flow
  l = log
  lc = log ORIG_HEAD.. --stat --no-merges
  lg = log -p
  m = merge --ff-only
  pl = pull
  p = push
  rc = rebase --continue
  s = status
  sh = show
  st = status
  t = tag
  add-patch = add --patch
  add-edit = add --edit
  lol = log --graph --decorate --pretty=format:\"%h %Cblue%aN%Creset %s\"
  llog = log --date=local
  unclean-working = diff --no-ext-diff --ignore-submodules --quiet --exit-code
  unclean-index = diff-index --cached --quiet --ignore-submodules HEAD --
  undo-commit = reset --soft HEAD^
  who = shortlog -s --
  changes=diff --name-status -r
  diffstat=diff --stat -r
  whois = "!sh -c 'git log -i -1 --pretty=\"format:%an <%ae>\n\" --author=\"$1\"' -"
  whatis = show -s --pretty='tformat:%h (%s, %ad)' --date=short
  graphviz = "!f() { echo 'digraph git {' ; git log --pretty='format:  %h -> { %p }' \"$@\" | sed 's/[0-9a-f][0-9a-f]*/\"&\"/g' ; echo '}'; }; f"
[merge]
  defaultToUpstream = true
[push]
  default = matching
[user]
	name = Brian Olsen
	email = brian@maven-group.org
  signingKey = 348182DC0E06A8FC1F01FB3AE95F7F83D78B7DAC
[gitflow "branch"]
  master = master
  develop = develop
[gitflow "prefix"]
  feature = feature/
  release = release/
  hotfix = hotfix/
  support = support/
  versiontag = v
[difftool]
  prompt = false
[core]
	excludesfile = /Users/bro/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[include]
    path = .gitconfig_host
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	gpgsign = true
